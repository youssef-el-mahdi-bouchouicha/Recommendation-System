<owl:Class rdf:about="#risk_register">
  <rdfs:subClassOf rdf:resource="#use"/>
  <rdfs:subClassOf rdf:resource="#document"/>
  <rdfs:subClassOf rdf:resource="#control_risks_inputs"/>
  <rdfs:subClassOf rdf:resource="#perform_quantitative__risk_analysis_inputs"/>
