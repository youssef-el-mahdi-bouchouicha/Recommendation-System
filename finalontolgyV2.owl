  <owl:Class rdf:about="http://example.org/projectOntology/schedule_compression">
    <rdfs:subClassOf rdf:resource="http://example.org/projectOntology/develop_schedule"/>
    <pr:uses rdf:resource="http://example.org/projectOntology/schedule_compression_uses"/>
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  are used to find ways to bring project activities that are behind into alignment with the plan by fast tracking or crashing the schedule for the remaining work.</rdfs:isDefinedBy>
    <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">None</rdfs:seeAlso>
